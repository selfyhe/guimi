/* pages/subUser/feedback/feedback.wxss */
page{
  background-color:white;
}
.container{
  width:100%;
  height:auto;
  position:relative;
}
.container_content{
  width:88%;
  height:auto;
  margin:0 auto;
  padding:60rpx 0 60rpx 0;
}

.container_title{
  color:#333333;
  font-size:32rpx;
}
.container_text{
  width:100%;
  background:rgba(246,246,246,1);
  margin-top:28rpx;
}

.container_text textarea{
    color:black;
    caret-color:black;
    padding: 20rpx 30rpx;
    width: 100%;
    height:240rpx;
    box-sizing: border-box;
}
/* 分割线 */
.container_division{
  width:100%;
  height:2rpx;
  background:rgba(246,246,246,1);
}
.container_content_text{
  font-size:26rpx;
  color:#999999;
  padding-top:15rpx;
}
.container_photo{
  width:100%;
  height:auto;
  padding-top:40rpx;
  margin-bottom:90rpx;
  overflow:hidden;
}
.photo_content{
  width:200rpx;
  height:200rpx;
  background:#F6F6F6;
   display: flex;
   float:left;
   margin-right:15rpx;
   margin-bottom:20rpx;
}
.photo_icon{
  box-sizing:border-box;
  width:56rpx;
  height:56rpx;
  margin:auto;
}
.container_button{
  width:88%;
  height:88rpx;
  background:rgba(162,208,133,1);
  border-radius:0px 0px 16rpx 16rpx;
  position:fixed;
  left:40rpx;
  bottom:32rpx;
  color:white;
  font-size:32rpx;
  text-align:center;
  line-height:88rpx;
}