/* pages/subUser/myCustom/customDetail.wxss */
@import '/pages/subUser/myOrder/orderDetail.wxss';
page{
  background:white;
}
.container_button{
  width:100%;
  height:110rpx;
  background:rgba(162,208,133,1);
  border-radius:20px 20px 0px 0px;
  position:fixed;
  bottom:0;
  left:0;
}
.container_box{
  width:80%;
  height:100%;
  margin:0 auto;
  display:flex;
  justify-content:space-between;
}
.container_money{
 width:auto;
 height:100%;
 color:white;
 display:flex;
 line-height:110rpx;
}
.money_text{
 font-size:28rpx;
}
.money{
  font-size:44rpx;
}
.container_pay{
  width:228rpx;
  height:72rpx;
  color:white;
  font-size:28rpx;
  line-height:72rpx;
  border-radius:36px;
  margin:auto 0 auto 0;
  text-align:center;
  border:2rpx solid white;
}
.lii{
  padding-bottom:40rpx;
   border-bottom:1px dotted #D9D9D9;
}
.ul{
   border-bottom:none;
}
.gongji{
  width:100%;
  height:80rpx;

}
.gongji{
  width:100%;
  height:80rpx;
  text-align:right;
  display:flex;
  padding-bottom:20rpx;
 justify-content:flex-end;
  /* border:1rpx solid red; */
}
.gongtext{
  font-size:28rpx;
  color:#333333;
  float:right;
  line-height:60rpx;
}
.gongnum{
  color:#F55A32;
  float:right;
  display:flex;
}