/* pages/exchange/myRedbao.wxss */
.container{
  padding: 30rpx 40rpx;
}
/* 同myVoucher.wxss一致 */
.card{
  position: relative;
  box-sizing: border-box;
  width: 100%;
  padding: 40rpx 0;
  background: #fff;
  margin-bottom:30rpx; 
  border-radius: 24rpx;
  /* box-shadow:0px 1px 5px 0px rgba(217,217,217,1); */
}
.card::before{
  position: absolute;
  content: "";
  top: 150rpx;
  left: 0;
  width: 100%;
  height: 50rpx;
  background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.card-hd{
  padding-left: 76rpx;
  font-size: 32rpx;
  color: #F97140;
  display: flex;
  justify-content: flex-start;
  align-items: flex-end;
  margin-bottom: 80rpx;
}
.card-price{
  font-size: 72rpx;
  color: #F55A32;
  margin-right: 12rpx;
  font-weight: bold;
}
.card-toprice{
  padding-bottom: 14rpx;
}
.card-bd{
  padding-left: 86rpx;
}
.card-title{
  font-size: 28rpx;
  color: #666;
  margin-bottom: 10rpx;
}
.card-txt{
  font-size: 28rpx;
  color: #999;
}
/*  */
.dialog-info{
  padding: 30rpx 0 50rpx;
  text-align: center;
  font-size: 32rpx;
  color: #666;
}