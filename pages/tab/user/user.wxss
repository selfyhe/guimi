.icon-addres{
  width: 38rpx;
  height: 44rpx;
}
.icon-custom{
  width: 40rpx;
  height: 40rpx;
}
.icon-service{
  width: 42rpx;
  height: 32rpx;
}
.icon-invoice{
  width: 36rpx;
  height: 42rpx;
}
/*  */
.userInfo-box{
  box-sizing: border-box; 
  position: relative;
  width: 100%;
  height: 230rpx;
  background: #A2D085;
}
.userInfo-inner{
  position: absolute;
  top: 24rpx;
  left: 0;
  z-index: 20;
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  padding-top: 24rpx;
}
.userInfo-box::before{
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  box-sizing: border-box; 
  width: 100%;
  height: 44rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  /* border-bottom-left-radius: 25%; */
  /* border-bottom-right-radius: 25%; */
  /* background: #5CAE32;  */
  background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.userInfo-inner{
  padding: 0 40rpx;
  box-sizing: border-box;
}
.userInfo-avatar{
  width: 112rpx;
  height: 112rpx;
  margin-right: 40rpx;
  border-radius: 50%;
  overflow: hidden;
}
.userInfo-bd{
  color: #fff;
  flex: 1;
}
.userInfo-name{
  font-size: 36rpx;
  font-weight: bold;
  margin-bottom: 5rpx;
}
.userInfo-phone{
  font-size: 30rpx;
}
/*  */
.ul{
  box-sizing: border-box;
  padding: 0 40rpx;
  height: 150rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background: #fff;
}
.li{
  flex: 1;
  text-align: center;
}
.li-title{
  font-size: 40rpx;
  font-weight: bold;
  color: #333;
}
.li-txt{
  color: #666;
  font-size: 26rpx;
}
/*  */
.panel{
  /* padding: 0 40rpx; */
  background: #fff;
}
.panel-hd{
  padding: 30rpx 40rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.panel-hd-title{
  color: #000;
  font-size: 36rpx;
  font-weight: bold;
}
.panel-hd-more{ 
  color: #666;
  font-size: 28rpx;
}
.panel-bd{
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.panel-grid{
  /* min-width: 80rpx;
  display: inline-block;
  margin: 0 30rpx; */
  flex: 1;
  text-align: center;
  position: relative;
}
.panel-grid-img{ 
  width: 56rpx;
  height: 56rpx;
  display: inline-flex;
  align-items: center;
  justify-content: center; 
  margin-bottom: 20rpx;
}
.panel-grid-title{
  font-size: 24rpx;
  color: #333;
}
/*  */
.cells{
  box-sizing: border-box;
  width: 100%;
  background: #fff;
  padding: 0 40rpx;
  /* padding-left: 30rpx;
  padding-right: 30rpx; */
}
.cell>navigator{
  box-sizing: border-box;
  width: 100%;
  height: 110rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  background: #fff;
}
.cell-hd,.cell-bd{
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.cell-bd{
  flex: 1;
}
.cell-p{
  font-size: 32rpx;
  color: #333;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
/*  */
.ad-box{ 
  padding: 40rpx 45rpx 10rpx 45rpx;
  overflow:hidden;
  white-space:nowrap;
  background: #fff;
  /* border:1rpx solid red; */
}
.ad_box_after{
  width:100%;
  height:auto;
  display: flex;
  justify-content: space-between;
  /* border:1rpx solid yellow; */
}
.ad-item{
  display: inline-block; 
  /* width: 300rpx;
  height: 160rpx; */
  width:49%;
  height:160rpx;
  /* margin-right: 30rpx; */
}
/*  */
button{
  padding: 0;
  margin: 0;
  background-color: transparent; 
  border-radius: 0; 
  line-height: inherit;
  font-size: inherit;
  text-align: inherit;
  color: inherit; 
}
.panel-hd-more button{
  position: absolute;
  width: 100%;
  height: 40rpx;
}