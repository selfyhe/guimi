.container{

}

.badge{
  position: absolute;
  top: 0;
  right: 10rpx;
  font-size: 26rpx;
  padding: 0 8rpx;
  min-width: 16rpx;
  height: 32rpx;
  line-height: 32rpx;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  color:#fff;
  background:#FF0000;
  border-radius: 16rpx;
}
.icon-inner{ 
  width: 44rpx;
  height: 44rpx;
  display: inline-flex;
  align-items: center;
  justify-content: center;
}

